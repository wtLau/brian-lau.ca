    "babel-core": "^6.26.3",
    "clsx": "^1.1.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "flow-bin": "^0.98.1",
    "globby": "^11.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.4.0",
    "prettier": "^2.2.1",
    "rehype": "^11.0.0",
    "typescript": "^4.2.2"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev  -p 3001",
    "build": "next build",
    "start": "next start",
    "find:unused": "next-unused",
    "test:coverage": "CI=1 yarn jest --coverage",
    "test": "yarn jest --watch --collectCoverage --silent",
    "lint": "next lint --fix --ext .js,.ts,.tsx ./ --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "yarn run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "yarn run prettier -- --list-different",
    "postinstall": "husky install",
    "precommit": "yarn check-types & yarn lint-staged"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "lint-staged": {
    "*.+(ts|tsx|js)": [
      "yarn prettier"
    ]
  }
}
